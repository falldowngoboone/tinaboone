  "scripts": {
    "browsersync": "browser-sync start --server --files '**/*.html, css/**/*.css, js/**/*.js, images/**/*'",
    "sass:watch": "node-sass --watch css -o css",
    "sass": "node-sass css -o css",
    "start": "run-p sass:watch browsersync",
    "build": "yarn sass",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.23.6",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2"
  }
